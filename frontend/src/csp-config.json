{
  "content-security-policy": {
    "default-src": ["'self'"],
    "script-src": [
      "'self'",
      "'unsafe-inline'",
      "'unsafe-eval'",
      "https://maps.googleapis.com",
      "https://maps.gstatic.com"
    ],
    "style-src": [
      "'self'",
      "'unsafe-inline'",
      "https://fonts.googleapis.com",
      "https://maps.googleapis.com",
      "https://maps.gstatic.com"
    ],
    "img-src": [
      "'self'",
      "data:",
      "blob:",
      "https:",
      "https://maps.googleapis.com",
      "https://maps.gstatic.com",
      "https://streetviewpixels-pa.googleapis.com"
    ],
    "font-src": [
      "'self'",
      "data:",
      "https://fonts.gstatic.com"
    ],
    "connect-src": [
      "'self'",
      "https://maps.googleapis.com",
      "https://maps.gstatic.com",
      "https://us-central1-conference-party-app.cloudfunctions.net"
    ],
    "frame-src": [
      "'self'",
      "https://maps.google.com"
    ],
    "worker-src": [
      "'self'",
      "blob:"
    ],
    "object-src": ["'none'"],
    "base-uri": ["'self'"],
    "form-action": ["'self'"],
    "frame-ancestors": ["'none'"]
  },
  "permissions-policy": {
    "geolocation": "(self https://maps.googleapis.com)",
    "camera": "()",
    "microphone": "()",
    "payment": "()",
    "usb": "()",
    "magnetometer": "()",
    "gyroscope": "()",
    "accelerometer": "()"
  }
}