{
  "hosting": {
    "public": "frontend/src",
    "rewrites": [{ "source": "/api/**", "function": "api" }],
    "headers": [
      {
        "source": "**",
        "headers": [
          { "key": "Cross-Origin-Opener-Policy", "value": "same-origin-allow-popups" }
        ]
      }
    ]
  },
  "functions": {
    "source": "functions"
  }
}